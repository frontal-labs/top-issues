{
	"author": "Frontal Labs",
	"baseBranch": "main",
	"branches": ["main"],
	"tagFormat": "v${version}",
	"plugins": [
		[
			"@auto-it/conventional-commits",
			{
				"preset": "conventionalcommits"
			}
		],
		"@auto-it/released",
		"@auto-it/changelog",
		[
			"@auto-it/npm",
			{
				"publish": false
			}
		],
		"@auto-it/github"
	],
	"labels": [
		{
			"name": "release:major",
			"releaseType": "major"
		},
		{
			"name": "release:minor",
			"releaseType": "minor"
		},
		{
			"name": "release:patch",
			"releaseType": "patch"
		},
		{
			"name": "breaking",
			"releaseType": "major"
		}
	]
}
